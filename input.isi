programa
  
  declare a, b.
  texto palavra.

  # Comentarios 123 #

  leia(b).
  leia(a).
  leia(palavra).

  a := 1 + 2 * 3 / b. 
  
  # Aqui tem um comando de decisao #
  se ( a > b ) entao {
     escreva (a).
  }
  senao {
  	 escreva (b).
  }
  
  
  palavra := "abracadabra".
  escreva(palavra).
  
  enquanto ( a > b) {
  	 escreva ("a").
  }
  
  # Acabou #
fimprog.